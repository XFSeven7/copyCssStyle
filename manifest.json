{
  "manifest_version": 3,
  "name": "copyStyle",
  "version": "1.0",
  "description": "一键复制div的样式",
  "permissions": ["tabs", "storage","contextMenus"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "src/assets/logo.png"
    }
  },
  "icons": {
    "48": "src/assets/logo.png"
  },
  "background": {
    "service_worker": "src/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/jquery.js","src/content.js"],
      "css": ["src/content.css"]
    }
  ]
}
